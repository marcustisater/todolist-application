<h1>Listing todoit_lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Text</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @todoit_lists.each do |todoit_list| %>
      <tr id="<%= dom_id(todoit_list) %>">
        <%= todoit_list.title %>
        <%= todoit_list.description %>
        <%= todoit_list.text %>
        <%= link_to "List Items", todoit_list_todoit_items_path(todoit_list) %>
        <%= link_to 'Show', todoit_list %>
        <%= link_to 'Edit', edit_todoit_list_path(todoit_list) %>
       <%= link_to 'Destroy', todoit_list, method: :delete, data: { confirm: 'Are you sure?' } %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Todoit list', new_todoit_list_path %>
